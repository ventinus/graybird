# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

# Ignore .vagrant directory
.vagrant

# Ignore coverage directory
coverage

.DS_Store

.byebug_history

# ignore test images
/public/system/dragonfly
/public/uploads
/public/packs
/public/packs-test
/spec/temp
/node_modules
